page{
  background-color: #FAFAFA;
}
.Receiving {
  background-color: #ffffff;
  padding:30rpx;
}
.userInfo{
  font-size: 38rpx;
  color: #403D44;
  line-height: 80rpx;
}
.userAdr{
  color: #7C7C7C;
  font-size: 34rpx;
}
.saleInfo{
  margin-top: 30rpx;
  padding:20rpx 30rpx;
  background-color: #FFFFFF;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 35rpx;
  color: #7C7C7C;
}
.saleImg{
  width: 50rpx;
  height: 50rpx;
}
.orderFoods{
  padding: 0 30rpx;
}
.orderFood{
  height:100rpx;
  line-height: 100rpx;
  margin: 0 auto;
  border-bottom: 2rpx solid #dddddd;
  display: flex;
  font-size: 34rpx;
  color: #000000;
  position: relative;
}
.foodName{
  position: absolute;
  left: 0;
}
.foodNum{
  color: #909090;
  position: absolute;
  left: 50%;
}
.foodPri{
  position: absolute;
  right: 0;
}
.total{
  display: flex;
  align-items: center;
  justify-content: flex-end;
  height: 100rpx;
  margin: 0 auto;
  font-size: 35rpx;
  padding-right: 40rpx;
  background-color: #ffffff;
}
.totalPri{
  margin-left: 30rpx;
  color: #ff4949;
}
.Remarks{
  font-size: 35rpx;
  margin-top: 30rpx;
  padding: 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background-color: #ffffff;
}
.Explain{
  background-color: #FFE9CC;
  margin-top: 30rpx;
  padding: 15rpx 0;
  display: flex;
  justify-content: center;
}
.contaniners{
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #A38052;
}
.PayBtns{
  display: flex;
  align-items: center;
}
.PayBtn{
  width: 30%;
  line-height: 2;
  margin-top: 80rpx;
  font-size: 36rpx;
  padding: 5rpx 30rpx;
  border: none;
  color: #ffffff;
}
.bgBlue{
  background-color: #4DA3D2;
}
.bgGray{
  background-color: #999999;
}
.PayBtn::after{
  border: none;
}
.refund{
  border:1rpx solid #4DA3D2;
  color: #4DA3D2;
  width: 30%;
  line-height: 2;
  margin-top: 80rpx;
  font-size: 36rpx;
  padding: 5rpx 30rpx;
}
.refund::after{
  border: none;
}
.modifyModal{
 width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  overflow: hidden;
  z-index: 90;
}
.modifyTel{
  width: 70%;
  height: 530rpx;
  overflow: hidden;
  position: fixed;
  top: 20%;
  left: 15%;
  z-index: 99;
  background: #ffffff;
  border-radius: 15rpx;
  padding-bottom: 10rpx;
}
.returnTie{
  border-radius: 15rpx 15rpx 0rpx 0rpx;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  background-color: #FF8049;
  color: #ffffff;
  font-size: 35rpx;
}
.pleaseInt{
  padding: 20rpx;
  font-size: 35rpx;
}
.refunds{
  width: 90%;
  margin: 0 auto;
  height: 230rpx;
  border: 1rpx solid #dddddd;
}
.clickBtn{
  width: 150rpx;
  padding: 0rpx;
  font-size: 35rpx;
  line-height: 2;
 background-color: #FF8049;
  color: #ffffff;
}
.newMessage{
  position: fixed;
  z-index: 9999;
  top: 50%;
  left: 34%;
  min-width: 250rpx;
  text-align: center;
  height: 45rpx;
  line-height: 45rpx;
  font-size: 35rpx;
  padding: 10rpx 20rpx;
  border-radius: 10rpx;
  background-color: rgba(0, 0, 0, 0.4);
  color: #ffffff;
}
.returnMoney{
  width: 80%;
  line-height: 2;
  background: #FF8049;
  color: #ffffff;
  font-size: 36rpx;
  margin-top: 40rpx;
}
.returnMoney::after{
  border: none;
}
.Remarkss{
  font-size: 35rpx;
  padding:20rpx 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background-color: #ffffff;
}