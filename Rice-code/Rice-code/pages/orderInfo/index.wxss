.userInfos{
  padding: 20rpx 30rpx 0rpx;
}
.userInfo{
  display: flex;
  align-items: center;
  height: 80rpx;
  padding: 10rpx 0rpx;
}
.infoHead{
  font-size: 35rpx;
  width: 25%;
  height: 100%;
  line-height: 80rpx;
}
.addInfo{
  display: flex;
  align-items: center;
  height: 100%;
  width: 75%;
}
.addName{
  width: 100%;
  font-size: 35rpx;
  height: 100%;
}
.addBorder{
  border-bottom: 1rpx solid #DDDDDD
}
.addBorders{
  border-bottom: 1rpx solid #F2F2F2
}
.addTel{
  width: 65%;
  font-size: 35rpx;
  height: 100%;
}
